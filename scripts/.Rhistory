dat.mod$CO2_surf_emis_sd[dat.mod$temp == 20 & dat.mod$gas == 'air'] <- sqrt((dat.mod$CO2_emis_sd[dat.mod$temp == 20 & dat.mod$gas == 'air'])^2 + (dat.mod$CO2_emis_sd[dat.mod$temp == 20 & dat.mod$gas == 'n2'])^2)
