cum_CO2 = cumsum(vol_co2)/bio_wet_weight * weights_scale * rho_CO2/1000, cum_CO2_vs = cumsum(vol_co2)/vs_mass_load * rho_CO2/1000) %>%
group_by(temp, gas, datetime) %>%
