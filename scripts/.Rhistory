dat.mod$CO2_surf_emis_mean[dat.mod$temp == 20 & dat.mod$gas == 'air'] <- dat.mod$CO2_emis_mean[dat.mod$temp == 20 & dat.mod$gas == 'air'] - dat.mod$CO2_emis_mean[dat.mod$temp == 20 & dat.mod$gas == 'n2']
