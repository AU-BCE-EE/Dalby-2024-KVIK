mutate_all(~ ifelse(is.na(.), 0, .), ~ ifelse(.<0, 0, .)) %>%
mutate(weights_scale = rep(weights_scale$weights_scale, length = nrow(dat_biogas[dat_biogas$day >= 283,]))) %>%
